<% team = ["Seb","Anne", "Romain"] %>
<% avatar = ["seb.jpg","anne.jpg", "romain.jpeg"] %>

<div class="container text-center">
  <div class="row">
    <% data.flats.each do |owner, flat| %>
      <div class="col-md-4">
        <div class="card">
          <h3><%= owner.capitalize %></h3>
          <%= image_tag flat.owner_picture, class:"card-user avatar avatar-large" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
